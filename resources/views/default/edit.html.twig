{% extends 'base.html.twig' %}

{% block body %}
    <h2>Edit configuration</h2>
    <form method="POST" action="{{ path('config_update') }}" class="form form-horizontal">
        {{ form_rest(form) }}

        <div class="form-actions">
            <button type="submit" class="btn btn-primary">Save</button>
        </div>
    </form>
{% endblock %}
